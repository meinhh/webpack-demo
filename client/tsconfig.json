{
	"exclude": [
		"*node_modules*"
	],
	"compilerOptions": {
		"target": "es5",
		"module": "amd",
		"allowJs": false,
		"declaration": false,
		"noImplicitAny": false,
		"removeComments": false,
		"noLib": false,
		"forceConsistentCasingInFileNames": true,
		"sourceMap": true,
		"baseUrl": ".",
		"moduleResolution": "node"
	}
}